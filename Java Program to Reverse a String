public class reverseString {

	public static void main(String[] args) {
		 String s = "Welcome to Banglore";
	        String[] str1   = s.split(" ");
	    String revWord = "";
	    
	    for(int i=str1.length-1;i>=0;i--){
	    
	        revWord =revWord.trim()+" "+str1[i];
	     
	    }
	  System.out.println(revWord);

	}

}
