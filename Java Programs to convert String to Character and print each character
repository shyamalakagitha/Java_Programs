class stringToChar {
  public static void main(String[] args) {
 String s = "testing";
  
  //One way
char[] c= s.toCharArray(); 
  
  for(int i=0; i<c.length;i++){
  //System.out.println(c[i]);
  }
  
  
  //Second way
  
  for(int i=0;i<s.length();i++){
    
     System.out.println(s.charAt(i));
  }
  
  
  }   
}
