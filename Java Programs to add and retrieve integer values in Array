class intArray {
  public static void main(String[] args) {
 int[] intArray = { 1,4,3,2,6,7,5,8,9,10}; 
  
  // How to Integer Array
  for(int i=0;i<intArray.length;i++){
  
  // How to retrieve all values from Integer array
     System.out.println(intArray[i]);
    
  
  }
//to find last 3rd number:
   System.out.println("Find the last 3rd number:  " + intArray[intArray.length-3]);    
}
    
    
  }
